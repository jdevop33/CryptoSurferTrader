# 8Trader8Panda Environment Configuration
# Copy this file to .env and fill in your API keys

# Trading Configuration
NODE_ENV=development
PORT=5000
MAX_POSITIONS=5
POSITION_SIZE_USD=100

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/trading_db
REDIS_URL=redis://localhost:6379

# API Keys (Required for full functionality)
TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here
ALIBABA_CLOUD_API_KEY=your_alibaba_cloud_api_key_here
ALCHEMY_API_KEY=your_alchemy_api_key_here
COINGECKO_API_KEY=your_coingecko_api_key_here

# Alibaba Cloud Deployment (Required for production)
ALIBABA_CLOUD_ACCESS_KEY=your_alibaba_access_key_here
ALIBABA_CLOUD_SECRET_KEY=your_alibaba_secret_key_here
ALIBABA_CLOUD_REGION=ap-southeast-1

# Trading Engine Configuration
AUTO_TRADING_ENABLED=false
MIN_SENTIMENT_SCORE=0.8
STOP_LOSS_PERCENTAGE=15
TAKE_PROFIT_PERCENTAGE=30

# Security
JWT_SECRET=your_jwt_secret_here
SESSION_SECRET=your_session_secret_here

# Ethereum/Web3 Configuration
ETHEREUM_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/your_key_here
INFURA_PROJECT_ID=your_infura_project_id_here

# Production Domain (for deployment)
DOMAIN_NAME=8trader8panda8.xin
SSL_ENABLED=true